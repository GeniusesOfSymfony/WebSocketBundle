{
    "name": "gos/web-socket-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Web Socket Bundle",
    "keywords": ["Web Socket Bundle", "Websocket", "WAMP", "IO", "Ratchet"],
    "homepage": "https://github.com/GeniusesOfSymfony/WebSocketBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Dare",
            "email": "jeremy.d.dare@gmail.com"
        },
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        },
        {
            "name": "Mathieu Delisle",
            "email": "mathieu@delisle.com",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^4.0",
        "gos/pubsub-router-bundle": "dev-master",
        "gos/websocket-client": "^0.1",
        "gos/pnctl-event-loop-emitter" : "^0.1",
        "ocramius/proxy-manager": "^2.2",
        "cboden/ratchet": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": { "Gos\\Bundle\\WebSocketBundle\\": "" }
    },
    "suggest": {
      "ext-libevent": ">=0.0.5",
      "ext-libev": "*",
      "ext-zmq": "*",
      "ext-amqp": "*",
      "symfony/proxy-manager-bridge": "~2.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
